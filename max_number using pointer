#include<iostream>
using namespace std;
int main()
{
	int a, b;
	cout << "please enter the 2 numbers\n";
	cin >> a >> b;
	int* ptrA = &a;
	int* ptrB = &b;
	if (*ptrA > * ptrB)
	{
		cout << "The maximum number is" << *ptrA << endl;
	}
	else if (*ptrA < *ptrB)
	{
		cout << "The maximum number is" << *ptrB << endl;
	}
	else
	{
		cout << "the two numbers are equal " <<  endl;
	}
	system("pause");
	return 0;
}
